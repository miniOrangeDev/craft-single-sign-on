{
    "name": "miniorangedev/craft-single-sign-on",
    "description": "Craft Single Sign-On OAuth & SAML Connect plugin allows unlimited sso / login ( Single Sign On ) with your Azure AD, Discord, G Suite / Google Apps or other custom SAML, OAuth 2.0, OpenID Connect providers.",
    "type": "craft-plugin",
    "version": "4.0.5",
    "keywords": [
        "SSO",
        "single-sign-on",
        "SAML",
        "OAuth",
        "JWT"
    ],
    "support": {
        "docs": "https://github.com/miniOrangeDev/craft-single-sign-on/blob/master/README.md",
        "issues": "https://github.com/miniOrangeDev/craft-single-sign-on/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "miniorange",
            "homepage": "https://github.com/miniorangedev"
        }
    ],
    "require": {
        "craftcms/cms": "3.0.0"
    },
    "require-dev": {
      "codeception/codeception": "^3.0",
      "vlucas/phpdotenv": "^3.0"
    },
    "autoload": {
        "psr-4": {
          "miniorangedev\\craftsinglesignon\\": "src/"
        }
    },
    "extra": {
        "name": "Single Sign-On",
        "handle": "craft-single-sign-on",
        "developer": "miniorange",
        "developerUrl": "https://github.com/miniorangedev",
        "documentationUrl": "https://github.com/miniOrangeDev/craft-single-sign-on/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/miniOrangeDev/craft-single-sign-on/master/CHANGELOG.md",
        "components": {
            "app": "miniorangedev\\craftsinglesignon\\services\\App"
        },
        "class": "miniorangedev\\craftsinglesignon\\Craftsinglesignon"
    }
}
